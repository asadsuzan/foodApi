{
     "version": 2,
     "builds": [
          {
               "src": "./index.js",
               "use": "@vercel/node"
          }
     ],
     "routes": [
          {
               "src": "/api/v1/(.*)",
               "dest": "/src/route/api.js"
          },
          {
               "src": ".*",
               "dest": "./index.js"
          }
     ]
}